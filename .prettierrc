{
  "singleAttributePerLine": true,
  "plugins": ["prettier-plugin-tailwindcss"],
  "htmlWhitespaceSensitivity": "ignore",
  "printWidth": 80,
  "vueIndentScriptAndStyle": true,
  "tailwindConfig": "./tailwind.config.js",
  "endOfLine": "lf",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "vueTemplateIndentScriptAndStyle": true,
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "parser": "vue"
      }
    },
    {
      "files": "*.css",
      "options": {
        "parser": "css"
      }
    },
    {
      "files": "*.json",
      "options": {
        "parser": "json"
      }
    }
  ]
}